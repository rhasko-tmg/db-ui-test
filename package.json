{
  "name": "db-ui-test",
  "description": "",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vitebook:dev": "vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx,.vue . --ignore-path .gitignore",
    "format": "run-s format:*",
    "format:eslint": "npm run lint:eslint -- --fix",
